{
  "ConnectionStrings": {
    "ProjectDbConnection": "Data Source=(localdb)\\mssqllocaldb;Initial Catalog=RestrictionLimitPopupDb;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "File": {
    "DefaultLogLevel": "Information",
    "MaxFileSizeInMB": 1024,
    "RetainFileCount": 100,
    "WriteIntervalInMilliSeconds": 1000,
    "Folder": "./wwwroot/logs",
    "FileOutputType":  "CSV"
  },
  "AllowedHosts": "*"
}
